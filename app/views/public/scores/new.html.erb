  <div class="d-flex justify-content-center">
    <%= form_with model: @score, url: scores_path do |f| %>
        <table class="admin_score_info score_form">
          <thead>
            <tr>
              <th></th>
              <th></th>
              <th></th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <th><%= f.label :name, "曲名" %></th>
              <td><%= f.text_field :name %></td>
            </tr>
            <tr>
              <th><%= f.label :artist, "歌手" %></th>
              <td><%= f.text_field :artist %></td>
            </tr>
            <tr>
              <th><%= f.label :site_id, "楽譜購入サイト" %></th>
              <td class="select_field"><%= f.collection_select :site_id, Site.all, :id, :name %></td>
            </tr>
            <tr>
              <th><%= f.label :member, "グループ構成人数" %></th>
              <td>
                <%= f.radio_button :member, Score.members.key(0) %><%= f.label :member, Score.members_i18n[:three], value: Score.members.key(0) %>
                <%= f.radio_button :member, Score.members.key(1) %><%= f.label :member, Score.members_i18n[:four], value: Score.members.key(1) %>
                <%= f.radio_button :member, Score.members.key(2) %><%= f.label :member, Score.members_i18n[:five], value: Score.members.key(2) %>
                <%= f.radio_button :member, Score.members.key(3) %><%= f.label :member, Score.members_i18n[:six], value: Score.members.key(3) %>
                <%= f.radio_button :member, Score.members.key(4) %><%= f.label :member, Score.members_i18n[:seven_or_more], value: Score.members.key(4) %>
                <%= f.radio_button :member, Score.members.key(5) %><%= f.label :member, Score.members_i18n[:boys], value: Score.members.key(5) %>
                <%= f.radio_button :member, Score.members.key(6) %><%= f.label :member, Score.members_i18n[:girls], value: Score.members.key(6) %>
              </td>
            </tr>
            <tr>
              <th><%= f.label :difficulty, "難易度" %></th>
              <td>
                <%= f.radio_button :difficulty, Score.difficulties.key(0) %><%= f.label :difficulty, Score.difficulties_i18n[:introduction], value: Score.difficulties.key(0) %>
                <%= f.radio_button :difficulty, Score.difficulties.key(1) %><%= f.label :difficulty, Score.difficulties_i18n[:biginner], value: Score.difficulties.key(1) %>
                <%= f.radio_button :difficulty, Score.difficulties.key(2) %><%= f.label :difficulty, Score.difficulties_i18n[:intermediate], value: Score.difficulties.key(2) %>
                <%= f.radio_button :difficulty, Score.difficulties.key(3) %><%= f.label :difficulty, Score.difficulties_i18n[:advanced], value: Score.difficulties.key(3) %>
                <%= f.radio_button :difficulty, Score.difficulties.key(4) %><%= f.label :difficulty, Score.difficulties_i18n[:superlative], value: Score.difficulties.key(4) %>
              </td>
            </tr>
            <tr>
              <th>アレンジ内容</th>
              <td>
                <%= f.collection_check_boxes(:arrange_ids, Arrange.all, :id, :body, include_hidden: false) do |arrange| %>
                  <%= arrange.label do %>
                    <%= arrange.check_box %>
                    <%= arrange.text %>
                  <% end %>
                <% end %>
              </td>
            </tr>
            <tr>
              <d><%= f.hidden_field :user_id, value: @user.id %></d>
            </tr>
          </tbody>
        </table>
        <div class="d-flex justify-content-center pl5">
          <%= f.submit "投稿", class: "btn btn-primary" %>
        </div>
    <% end %>
  </div>